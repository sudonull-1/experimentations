{
  "functions": {
    "api/backtest.py": {
      "runtime": "python3.9"
    }
  },
  "builds": [
    {
      "src": "frontend/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    { "handle": "filesystem" },
    { "src": "/api/backtest", "dest": "/api/backtest.py" },
    { "src": "/(.*)", "dest": "/frontend/$1" }
  ]
}
